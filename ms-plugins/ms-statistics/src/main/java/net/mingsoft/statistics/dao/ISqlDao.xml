<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.mingsoft.statistics.dao.ISqlDao">

	<resultMap id="resultMap" type="net.mingsoft.statistics.entity.SqlEntity">
			<id column="id" property="id" /><!--编号 -->
				<result column="name" property="name" /><!--统计名称 -->
				<result column="key" property="key" /><!--统计key值 -->
				<result column="type" property="type" /><!--统计类型 -->
				<result column="sql" property="sql" /><!--统计SQL -->
				<result column="create_by" property="createBy" /><!--创建人 -->
				<result column="create_date" property="createDate" /><!--创建时间 -->
				<result column="update_by" property="updateBy" /><!--修改人 -->
				<result column="update_date" property="updateDate" /><!--修改时间 -->
				<result column="del" property="del" /><!--删除标记 -->
	</resultMap>


</mapper>
