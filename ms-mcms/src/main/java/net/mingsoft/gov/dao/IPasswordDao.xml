<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.mingsoft.gov.dao.IPasswordDao">

	<resultMap id="resultMap" type="net.mingsoft.gov.entity.PasswordEntity">
			<id column="id" property="id" /><!--编号 -->
				<result column="password_type" property="passwordType" /><!--业务类型 -->
				<result column="password_owner_id" property="passwordOwnerId" /><!--关联用户 -->
				<result column="password_owner_pwd" property="passwordOwnerPwd" /><!--密钥 -->
				<result column="create_by" property="createBy" /><!--创建人 -->
				<result column="create_date" property="createDate" /><!--创建时间 -->
				<result column="update_by" property="updateBy" /><!--修改人 -->
				<result column="update_date" property="updateDate" /><!--修改时间 -->
				<result column="del" property="del" /><!--删除标记 -->
	</resultMap>

</mapper>
