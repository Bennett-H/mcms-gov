<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.mingsoft.approval.dao.IConfigDao">

	<resultMap id="resultMap" type="net.mingsoft.approval.entity.ConfigEntity">
			<id column="id" property="id" /><!--编号 -->
				<result column="scheme_id" property="schemeId" /><!--方案名称 -->
				<result column="progress_id" property="progressId" /><!--审批节点 -->
				<result column="config_role_ids" property="configRoleIds" /><!--等级角色配置 -->
				<result column="category_id" property="categoryId" /><!--栏目ID -->
				<result column="create_by" property="createBy" /><!--创建人 -->
				<result column="create_date" property="createDate" /><!--创建时间 -->
				<result column="update_by" property="updateBy" /><!--修改人 -->
				<result column="update_date" property="updateDate" /><!--修改时间 -->
				<result column="del" property="del" /><!--删除标记 -->
	</resultMap>


</mapper>
