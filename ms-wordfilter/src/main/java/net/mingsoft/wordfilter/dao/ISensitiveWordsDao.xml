<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="net.mingsoft.wordfilter.dao.ISensitiveWordsDao">

	<resultMap id="resultMap" type="net.mingsoft.wordfilter.entity.SensitiveWordsEntity">
			<id column="id" property="id" /><!--编号 -->
				<result column="word" property="word" /><!--敏感词 -->
				<result column="replace_word" property="replaceWord" /><!--替换词 -->
				<result column="word_type" property="wordType" /><!--词汇类型 -->
				<result column="create_by" property="createBy" /><!--创建人 -->
				<result column="create_date" property="createDate" /><!--创建时间 -->
				<result column="update_by" property="updateBy" /><!--修改人 -->
				<result column="update_date" property="updateDate" /><!--修改时间 -->
				<result column="del" property="del" /><!--删除标记 -->
	</resultMap>

</mapper>
